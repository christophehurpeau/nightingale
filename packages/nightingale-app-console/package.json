{
  "name": "nightingale-app-console",
  "version": "3.2.6",
  "description": "nightingale default config and logger for app",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-app-console"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=8.3.0"
  },
  "browserslist": ["> 0.1%", "Firefox ESR", "not ie < 9", "not dead"],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .ts --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "node",
        "version": "8",
        "formats": ["cjs"]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": ["es"]
      },
      {
        "target": "browser",
        "formats": ["cjs", "es"]
      }
    ],
    "entries": ["index"]
  },
  "dependencies": {
    "nightingale": "^9.2.3",
    "nightingale-browser-console": "^4.2.2",
    "nightingale-console": "^6.1.1",
    "source-map-support": "^0.4.14"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@pob/eslint-config-babel": "31.0.0",
    "@pob/eslint-config-typescript": "31.1.0",
    "@pob/version": "4.32.1",
    "@types/source-map-support": "0.4.2",
    "babel-preset-latest-node": "3.2.1",
    "babel-preset-modern-browsers": "14.0.0",
    "eslint": "5.16.0",
    "pob-babel": "23.18.0",
    "prettier": "1.18.2"
  }
}
