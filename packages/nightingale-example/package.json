{
  "name": "nightingale-example",
  "private": true,
  "version": "10.0.1",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-example"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=12.10.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./configure": {
      "node": {
        "development": {
          "require": "./dist/configure-node10-dev.cjs.js"
        },
        "require": "./dist/configure-node10.cjs.js"
      }
    },
    "./console": {
      "node": {
        "development": {
          "require": "./dist/console-node10-dev.cjs.js"
        },
        "require": "./dist/console-node10.cjs.js"
      }
    },
    "./debug": {
      "node": {
        "development": {
          "require": "./dist/debug-node10-dev.cjs.js"
        },
        "require": "./dist/debug-node10.cjs.js"
      }
    },
    "./errors": {
      "node": {
        "development": {
          "require": "./dist/errors-node10-dev.cjs.js"
        },
        "require": "./dist/errors-node10.cjs.js"
      }
    },
    "./jsonConsole": {
      "node": {
        "development": {
          "require": "./dist/jsonConsole-node10-dev.cjs.js"
        },
        "require": "./dist/jsonConsole-node10.cjs.js"
      }
    }
  },
  "sideEffects": true,
  "scripts": {
    "build": "pob-build",
    "lint:eslint": "yarn --cwd ../.. eslint --ext .js,.ts --report-unused-disable-directives --quiet packages/nightingale-example",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs"
        ]
      }
    ],
    "entries": [
      "configure",
      "console",
      "debug",
      "errors",
      "jsonConsole"
    ]
  },
  "dependencies": {
    "nightingale": "^10.0.1",
    "nightingale-console": "^10.0.1",
    "nightingale-console-output": "^10.0.1",
    "nightingale-json-formatter": "^10.0.1",
    "nightingale-string": "^10.0.1",
    "nightingale-types": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@pob/eslint-config": "41.0.3",
    "@pob/eslint-config-node": "41.0.0",
    "@pob/eslint-config-typescript": "41.0.2",
    "@pob/eslint-config-typescript-node": "41.0.0",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "babel-preset-latest-node": "5.1.1",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "23.0.0",
    "pob-babel": "26.0.0",
    "rollup": "2.33.1",
    "typescript": "4.0.5"
  }
}
