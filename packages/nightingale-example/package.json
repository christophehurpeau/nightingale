{
  "name": "nightingale-example",
  "private": true,
  "version": "2.2.6",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-example"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:aliases-node": {
    "./configure.js": "./dist/configure-node10.es.js",
    "./console.js": "./dist/console-node10.es.js",
    "./debug.js": "./dist/debug-node10.es.js",
    "./errors.js": "./dist/errors-node10.es.js",
    "./jsonConsole.js": "./dist/jsonConsole-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./configure.js": "./dist/configure-node10-dev.es.js",
    "./console.js": "./dist/console-node10-dev.es.js",
    "./debug.js": "./dist/debug-node10-dev.es.js",
    "./errors.js": "./dist/errors-node10-dev.es.js",
    "./jsonConsole.js": "./dist/jsonConsole-node10-dev.es.js"
  },
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "lint": "eslint --ext .ts --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": ["cjs", "es"]
      },
      {
        "target": "node",
        "version": "8",
        "formats": ["cjs"]
      }
    ],
    "entries": ["configure", "console", "debug", "errors", "jsonConsole"]
  },
  "dependencies": {
    "nightingale": "^9.2.3",
    "nightingale-console": "^6.1.1",
    "nightingale-console-output": "^4.2.2",
    "nightingale-json-formatter": "^4.3.0",
    "nightingale-string": "^5.2.2",
    "nightingale-types": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@pob/eslint-config-babel": "31.0.0",
    "@pob/eslint-config-babel-node": "31.0.0",
    "@pob/eslint-config-typescript": "31.1.0",
    "@pob/version": "4.33.4",
    "babel-preset-latest-node": "3.2.1",
    "eslint": "5.16.0",
    "pob-babel": "23.22.2",
    "prettier": "1.19.1"
  }
}
