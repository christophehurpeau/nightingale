{
  "name": "nightingale-example",
  "private": true,
  "version": "13.0.0",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-example"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": {
    "./package.json": "./package.json",
    "./configure": {
      "node": {
        "import": "./build/configure-node16.mjs"
      }
    },
    "./console": {
      "node": {
        "import": "./build/console-node16.mjs"
      }
    },
    "./debug": {
      "node": {
        "import": "./build/debug-node16.mjs"
      }
    },
    "./errors": {
      "node": {
        "import": "./build/errors-node16.mjs"
      }
    },
    "./jsonConsole": {
      "node": {
        "import": "./build/jsonConsole-node16.mjs"
      }
    }
  },
  "module:aliases-node": {
    "./configure.js": "./build/configure-node16.es.js",
    "./console.js": "./build/console-node16.es.js",
    "./debug.js": "./build/debug-node16.es.js",
    "./errors.js": "./build/errors-node16.es.js",
    "./jsonConsole.js": "./build/jsonConsole-node16.es.js"
  },
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs",
    "clean": "yarn clean:build",
    "clean:build": "pob-babel-clean-out build",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/nightingale-example",
    "start": "yarn clean:build && rollup --config rollup.config.mjs --watch",
    "watch": "pob-watch"
  },
  "prettier": "@pob/root/prettier-config",
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "16"
      }
    ],
    "entries": [
      "configure",
      "console",
      "debug",
      "errors",
      "jsonConsole"
    ]
  },
  "dependencies": {
    "nightingale": "13.0.0",
    "nightingale-console": "13.0.0",
    "nightingale-console-output": "13.0.0",
    "nightingale-json-formatter": "13.0.0",
    "nightingale-string": "13.0.0",
    "nightingale-types": "13.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "pob-babel": "35.4.0",
    "typescript": "4.9.5"
  }
}
