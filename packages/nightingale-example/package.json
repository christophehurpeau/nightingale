{
  "name": "nightingale-example",
  "private": true,
  "version": "10.0.1",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-example"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:aliases-node": {
    "./configure.js": "./dist/configure-node10.es.js",
    "./console.js": "./dist/console-node10.es.js",
    "./debug.js": "./dist/debug-node10.es.js",
    "./errors.js": "./dist/errors-node10.es.js",
    "./jsonConsole.js": "./dist/jsonConsole-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./configure.js": "./dist/configure-node10-dev.es.js",
    "./console.js": "./dist/console-node10-dev.es.js",
    "./debug.js": "./dist/debug-node10-dev.es.js",
    "./errors.js": "./dist/errors-node10-dev.es.js",
    "./jsonConsole.js": "./dist/jsonConsole-node10-dev.es.js"
  },
  "scripts": {
    "build": "pob-build",
    "lint": "yarn --cwd ../.. eslint --ext .js,.ts --quiet packages/nightingale-example",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "configure",
      "console",
      "debug",
      "errors",
      "jsonConsole"
    ]
  },
  "dependencies": {
    "nightingale": "^10.0.1",
    "nightingale-console": "^10.0.1",
    "nightingale-console-output": "^10.0.1",
    "nightingale-json-formatter": "^10.0.1",
    "nightingale-string": "^10.0.1",
    "nightingale-types": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "babel-preset-latest-node": "4.1.0",
    "pob-babel": "24.12.1"
  }
}
