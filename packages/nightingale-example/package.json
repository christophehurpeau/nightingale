{
  "name": "nightingale-example",
  "private": true,
  "version": "11.0.0",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-example"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=12.10.0"
  },
  "main": "./index.js",
  "exports": {
    "./configure": {
      "node": {
        "development": {
          "import": "./dist/configure-node12-dev.mjs",
          "require": "./dist/configure-node12-dev.cjs.js"
        },
        "import": "./dist/configure-node12.mjs",
        "require": "./dist/configure-node12.cjs.js"
      }
    },
    "./console": {
      "node": {
        "development": {
          "import": "./dist/console-node12-dev.mjs",
          "require": "./dist/console-node12-dev.cjs.js"
        },
        "import": "./dist/console-node12.mjs",
        "require": "./dist/console-node12.cjs.js"
      }
    },
    "./debug": {
      "node": {
        "development": {
          "import": "./dist/debug-node12-dev.mjs",
          "require": "./dist/debug-node12-dev.cjs.js"
        },
        "import": "./dist/debug-node12.mjs",
        "require": "./dist/debug-node12.cjs.js"
      }
    },
    "./errors": {
      "node": {
        "development": {
          "import": "./dist/errors-node12-dev.mjs",
          "require": "./dist/errors-node12-dev.cjs.js"
        },
        "import": "./dist/errors-node12.mjs",
        "require": "./dist/errors-node12.cjs.js"
      }
    },
    "./jsonConsole": {
      "node": {
        "development": {
          "import": "./dist/jsonConsole-node12-dev.mjs",
          "require": "./dist/jsonConsole-node12-dev.cjs.js"
        },
        "import": "./dist/jsonConsole-node12.mjs",
        "require": "./dist/jsonConsole-node12.cjs.js"
      }
    }
  },
  "scripts": {
    "build": "pob-build",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. eslint --ext .js,.ts --report-unused-disable-directives --quiet packages/nightingale-example",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "12",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "configure",
      "console",
      "debug",
      "errors",
      "jsonConsole"
    ]
  },
  "dependencies": {
    "nightingale": "^11.0.0",
    "nightingale-console": "^11.0.0",
    "nightingale-console-output": "^11.0.0",
    "nightingale-json-formatter": "^11.0.0",
    "nightingale-string": "^11.0.0",
    "nightingale-types": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@pob/eslint-config": "42.0.1",
    "@pob/eslint-config-node": "42.0.0",
    "@pob/eslint-config-typescript": "42.0.1",
    "@pob/eslint-config-typescript-node": "42.0.0",
    "@rollup/plugin-run": "2.0.2",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "babel-preset-latest-node": "5.2.0",
    "eslint": "7.17.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "25.0.1",
    "pob-babel": "26.3.0",
    "rollup": "2.36.1",
    "typescript": "4.1.3"
  }
}
