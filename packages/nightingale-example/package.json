{
  "name": "nightingale-example",
  "private": true,
  "version": "15.0.0",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-example"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "type": "module",
  "engines": {
    "node": ">=20.9.0"
  },
  "exports": {
    "./package.json": "./package.json",
    "./configure": {
      "types": "./src/configure.ts",
      "node": {
        "import": "./build/configure-node20.mjs"
      }
    },
    "./console": {
      "types": "./src/console.ts",
      "node": {
        "import": "./build/console-node20.mjs"
      }
    },
    "./debug": {
      "types": "./src/debug.ts",
      "node": {
        "import": "./build/debug-node20.mjs"
      }
    },
    "./errors": {
      "types": "./src/errors.ts",
      "node": {
        "import": "./build/errors-node20.mjs"
      }
    },
    "./jsonConsole": {
      "types": "./src/jsonConsole.ts",
      "node": {
        "import": "./build/jsonConsole-node20.mjs"
      }
    },
    "./slack": {
      "types": "./src/slack.ts",
      "node": {
        "import": "./build/slack-node20.mjs"
      }
    }
  },
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs",
    "clean": "yarn clean:build",
    "clean:build": "pob-babel-clean-out build",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/nightingale-example",
    "start": "yarn clean:build && rollup --config rollup.config.mjs --watch",
    "watch": "pob-watch"
  },
  "prettier": "@pob/root/prettier-config",
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "20",
        "omitVersionInFileName": true
      }
    ],
    "entries": [
      "configure",
      "console",
      "debug",
      "errors",
      "jsonConsole",
      "slack"
    ]
  },
  "dependencies": {
    "nightingale": "15.0.0",
    "nightingale-console": "15.0.0",
    "nightingale-console-output": "15.0.0",
    "nightingale-json-formatter": "15.0.0",
    "nightingale-slack": "15.0.0",
    "nightingale-string": "15.0.0",
    "nightingale-types": "15.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "pob-babel": "38.0.1",
    "typescript": "5.3.3"
  }
}
