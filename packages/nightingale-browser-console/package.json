{
  "name": "nightingale-browser-console",
  "version": "4.2.2",
  "description": "Browser console handler for nightingale",
  "keywords": ["nightingale", "nightingale-handler"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-browser-console"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "browserslist": ["> 0.1%", "Firefox ESR", "not ie < 9", "not dead"],
  "main": "./dist/index-browser.es.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --ext .ts --quiet src/",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "browser",
        "version": "modern",
        "formats": ["es"]
      },
      {
        "target": "browser",
        "formats": ["cjs", "es"]
      }
    ],
    "entries": ["index"]
  },
  "dependencies": {
    "nightingale-browser-console-formatter": "^4.2.2",
    "nightingale-console-output": "^4.2.2",
    "nightingale-debug": "^5.2.2",
    "nightingale-types": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@pob/eslint-config-babel": "31.0.0",
    "@pob/eslint-config-typescript": "31.1.0",
    "@pob/version": "4.32.1",
    "babel-preset-modern-browsers": "14.0.0",
    "eslint": "5.16.0",
    "pob-babel": "23.19.0",
    "prettier": "1.18.2"
  }
}
