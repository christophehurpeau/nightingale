{
  "name": "nightingale-app-react-native",
  "version": "12.1.4",
  "description": "React Native and Expo logger for application",
  "keywords": [
    "nightingale",
    "logger",
    "expo",
    "react-native"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-app-react-native"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "browserslist": [
    "defaults",
    "> 0.2%",
    "not ie < 12",
    "not safari < 10",
    "not ios_saf < 10"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "browser": "./dist/index-browser.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "browser:modern": {
          "import": "./dist/index-browsermodern.es.js"
        },
        "import": "./dist/index-browser.es.js",
        "require": "./dist/index-browser.cjs.js"
      }
    }
  },
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "yarn clean:build",
    "clean:build": "rm -Rf dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "cd ../.. && yarn run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/nightingale-app-react-native",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "peerDependencies": {
    "react-native": "*",
    "react-native-web": "*"
  },
  "peerDependenciesMeta": {
    "react-native-web": {
      "optional": true
    }
  },
  "dependencies": {
    "nightingale": "12.1.4",
    "nightingale-browser-console": "12.1.4",
    "nightingale-react-native-console": "12.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@types/react-native": "0.67.3",
    "babel-preset-modern-browsers": "15.0.2",
    "pob-babel": "32.2.0",
    "typescript": "4.6.2"
  }
}
