{
  "name": "nightingale-app-react-native",
  "version": "11.7.1",
  "description": "React Native and Expo logger for application",
  "keywords": [
    "nightingale",
    "logger",
    "expo",
    "react-native"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-app-react-native"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "browserslist": [
    "defaults",
    "> 0.2%",
    "not ie < 12",
    "not safari < 10",
    "not ios_saf < 10"
  ],
  "main": "./dist/index-browser.es.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "browser": "./dist/index-browser.es.js",
  "exports": {
    ".": {
      "browser": {
        "browser:modern": {
          "development": {
            "import": "./dist/index-browsermodern-dev.es.js"
          },
          "import": "./dist/index-browsermodern.es.js"
        },
        "development": {
          "import": "./dist/index-browser-dev.es.js",
          "require": "./dist/index-browser-dev.cjs.js"
        },
        "import": "./dist/index-browser.es.js",
        "require": "./dist/index-browser.cjs.js"
      }
    }
  },
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. run eslint --ext .js,.mjs,.ts --report-unused-disable-directives --quiet packages/nightingale-app-react-native",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "peerDependencies": {
    "react-native": "*",
    "react-native-web": "*"
  },
  "peerDependenciesMeta": {
    "react-native-web": {
      "optional": true
    }
  },
  "dependencies": {
    "@types/node": ">=12.0.0",
    "nightingale": "^11.7.1",
    "nightingale-browser-console": "^11.7.1",
    "nightingale-react-native-console": "^11.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.9",
    "@pob/eslint-config": "43.2.0",
    "@pob/eslint-config-node": "43.2.0",
    "@pob/eslint-config-typescript": "43.2.0",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "babel-preset-modern-browsers": "15.0.2",
    "eslint": "7.32.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "29.0.0",
    "pob-babel": "27.0.3",
    "rollup": "2.53.1",
    "typescript": "4.3.5"
  }
}
