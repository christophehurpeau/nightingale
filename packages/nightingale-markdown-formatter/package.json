{
  "name": "nightingale-markdown-formatter",
  "version": "11.7.0",
  "description": "Nightingale Markdown formatter",
  "keywords": [
    "nightingale",
    "nightingale-formatter"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-markdown-formatter"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=12.10.0"
  },
  "browserslist": [
    "> 0.2%",
    "Firefox ESR",
    "last 2 Chrome versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Safari versions",
    "not ie < 12",
    "not ios_saf < 10",
    "not dead",
    "not op_mini all"
  ],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "browser": "./dist/index-browser.es.js",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node12-dev.mjs",
          "require": "./dist/index-node12-dev.cjs.js"
        },
        "import": "./dist/index-node12.mjs",
        "require": "./dist/index-node12.cjs.js"
      },
      "browser": {
        "browser:modern": {
          "development": {
            "import": "./dist/index-browsermodern-dev.es.js"
          },
          "import": "./dist/index-browsermodern.es.js"
        },
        "development": {
          "import": "./dist/index-browser-dev.es.js",
          "require": "./dist/index-browser-dev.cjs.js"
        },
        "import": "./dist/index-browser.es.js",
        "require": "./dist/index-browser.cjs.js"
      }
    }
  },
  "module:node": "./dist/index-node12.mjs",
  "module:node-dev": "./dist/index-node12-dev.mjs",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "sideEffects": false,
  "scripts": {
    "build": "pob-build && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=pob-lcov-reporter --coverageDirectory=docs/coverage/",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. run eslint --ext .js,.mjs,.ts --report-unused-disable-directives --quiet packages/nightingale-markdown-formatter",
    "test": "jest",
    "test:watch": "jest",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts",
      "<rootDir>/src/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "babel-jest"
    },
    "testEnvironment": "node"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "12",
        "formats": [
          "cjs",
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "nightingale-formatter": "^11.7.0",
    "nightingale-types": "^11.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@pob/eslint-config": "43.0.0",
    "@pob/eslint-config-node": "43.1.0",
    "@pob/eslint-config-typescript": "43.0.0",
    "@types/jest": "26.0.22",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "babel-jest": "26.6.3",
    "babel-preset-latest-node": "5.5.1",
    "babel-preset-modern-browsers": "15.0.2",
    "eslint": "7.24.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "25.0.1",
    "jest": "26.6.3",
    "nightingale-levels": "^11.7.0",
    "pob-babel": "26.8.0",
    "pob-lcov-reporter": "4.0.1",
    "rollup": "2.45.0",
    "typescript": "4.2.4",
    "xunit-file": "1.0.0"
  }
}
