{
  "name": "nightingale-sentry",
  "version": "5.1.0",
  "description": "Sentry handler for nightingale",
  "keywords": ["nightingale", "nightingale-handler"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-sentry"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "npm run typescript-check && eslint --ext .ts src/",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "@sentry/node": "^4.6.3",
    "nightingale-levels": "^4.1.0",
    "nightingale-types": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@typescript-eslint/eslint-plugin": "1.4.1",
    "@typescript-eslint/parser": "1.4.1",
    "babel-eslint": "10.0.1",
    "babel-preset-latest-node": "3.0.1",
    "eslint": "5.14.1",
    "eslint-config-pob": "28.0.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "pob-babel": "23.6.7",
    "prettier": "1.16.4",
    "typescript": "3.3.3333"
  }
}
