{
  "name": "nightingale-level-names",
  "version": "11.0.0",
  "description": "Nightingale level values to level names",
  "keywords": [
    "logger"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-level-names"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=12.10.0"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "yarn run build:definitions",
    "build:definitions": "tsc --lib esnext --noEmit ./lib/index.d.ts",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. eslint --report-unused-disable-directives --quiet packages/nightingale-level-names"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {},
  "devDependencies": {
    "@pob/eslint-config": "41.0.4",
    "@pob/eslint-config-node": "41.1.0",
    "eslint": "7.15.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "23.0.0"
  }
}
